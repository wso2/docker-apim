	                      	<li>
	                            <a title="" href="<%=jagg.url("/site/pages/add.jag")%>" onclick="jagg.sessionAwareJS({redirect:'<%=jagg.url("/info")%>?<%=apiUrlId%>',e:event,ssoEnabled:<%=ssoEnabled%>})">
