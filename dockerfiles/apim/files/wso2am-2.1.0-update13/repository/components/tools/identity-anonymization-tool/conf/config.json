{
  "processors" : [
    "log-file",
    "rdbms"
  ],
  "directories": [
    {
      "dir": "log-config",
      "type": "log-file",
      "processor" : "log-file",
      "log-file-path" : "logs",
      "log-file-name-regex" : "(.)*(log|out)"
    },
    {
      "dir": "sql",
      "type": "rdbms",
      "processor" : "rdbms"
    }
  ],
  "extensions": [
    {
      "dir": "datasources",
      "type": "datasource",
      "processor" : "rdbms",
      "properties" : [
        {
        "identity": "WSO2AM_DB",
        "apim": "WSO2AM_DB",
        "um": "WSO2_CARBON_DB"
        }
      ]
    }
  ]
}
